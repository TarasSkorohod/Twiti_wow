{
  "env": {
    "browser": true,
    "es2021": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "plugin:react/recommended",
    "standard",
    "prettier"
   
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": { 
    "react/prop-types": 0, 
    "no-unused-vars": "warn", 
    "prefer-const": "warn", 
    "padding-line-between-statements": ["error", { 
      "blankLine": "always", 
      "prev": "*", 
      "next": "return" 
    }], 
    "react/react-in-jsx-scope": "off", 
    "no-console": "warn" 
  } 
}